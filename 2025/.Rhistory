password <- function(x){
counter <- 0
dial <- 50
for(i in 1:length(x)){
direction <- str_sub(dial[i],1,1)
rotation <- as.numeric(str_sub(dial[1], start = 2))
rotation <- rotation %% 100
if(direction == "R"){
dial <- dial + rotation
} else {
dial <- dial - rotation
}
}
dial <- dial %% 100
if(dial == 0){
counter <- counter + 1
}
print(counter)
}
