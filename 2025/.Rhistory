batteries <- as.numeric(strsplit(bank, "")[[1]])
size <- length(batteries)
startPos <- 1
selectBatt <- 12
for(i in 1:12){
endPos <- size - (12 - i)
maxVal <- max(batteries[startPos:endPos])
maxPos <- startPos - 1 + which.max(batteries[startPos:endPos])
selectBatt[i] <- maxVal
startPos <- maxPos + 1
}
as.numeric(paste(selectBatt, collapse = ""))
}))
}
